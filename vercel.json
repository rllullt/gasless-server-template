{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "build": {
    "env": {
      "VERCEL_NPM_RUN_INSTALL": "false",
      "VERCEL_YARN_RUN_INSTALL": "true"
    },
    "installCommand": "yarn install"
  }
}
